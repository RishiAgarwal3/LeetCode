class Solution:
    def searchInsert(self, nums: List[int], target: int) -> int:
        if target in nums:
            for i in nums:
                if i == target:
                    return nums.index(i)
        b = 0
        if target not in nums:
            if max(nums)<target:
                return len(nums)
            else:
                for i in nums:
                    if i<target:
                        continue
                    elif i >target:
                        a = nums.index(i)
                        return a
